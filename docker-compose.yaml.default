version: "3.5"
services:
  satosa:
    build:
      context: .
      dockerfile: Dockerfile.centos
    image: local/satosa
    hostname: satosa
    networks:
      jenkins:
    # ports:
    #   - "8000:8000"
    restart: unless-stopped
    volumes:
      - satosa.opt_etc_satosa:/opt/etc/satosa/:Z

networks:
  jenkins:
    name: jenkins

volumes:
    satosa.opt_etc_satosa:
      name: satosa.opt_etc_satosa
